{
  "name": "wkt-parser-helper",
  "version": "4.1.0",
  "keywords": [
    "wkt",
    "GeoJSON",
    "GeometryCollection",
    "FeatureCollection",
    "well-known-text"
  ],
  "description": "Module to help parse GeoJSONs to WKT and back",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "url": "https://github.com/alrico88/wkt-parser-helper"
  },
  "author": "Alberto Rico",
  "license": "MIT",
  "dependencies": {
    "wellknown": "^0.5.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/geojson": "^7946.0.7",
    "@types/wellknown": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "c8": "^7.12.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.25.1",
    "tsup": "^6.2.1",
    "typedoc": "^0.20.30",
    "typedoc-plugin-markdown": "^3.6.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.5",
    "vitest": "^0.21.1"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "start": "pnpm run build -- --watch src",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.js src",
    "docs": "typedoc src/index.ts --readme README.md --gitRevision master"
  }
}